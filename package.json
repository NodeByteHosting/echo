{
    "name": "@nodebyte/echo",
    "description": "NodeByte's clever fox. Always curious, always connected, and always ready to lend a paw.",
    "version": "0.1.0",
    "type": "module",
    "main": "src/index.js",
    "imports": {
        "#classes": "./src/class/",
        "#classes/*": "./src/class/*",
        "#ai": "./src/echo-ai/",
        "#ai/*": "./src/echo-ai/*",
        "#configs": "./src/configs/",
        "#configs/*": "./src/configs/*",
        "#database": "./src/database/",
        "#database/*": "./src/database/*",
        "#functions": "./src/functions/",
        "#functions/*": "./src/functions/*",
        "#handlers": "./src/handlers/",
        "#handlers/*": "./src/handlers/*",
        "#utils": "./src/utils/",
        "#utils/*": "./src/utils/*"
    },
    "scripts": {
        "dev": "bun run --watch src/index.js",
        "start": "bun run src/index.js",
        "start:prod": "bun run src/index.js",
        "build": "prisma generate",
        "prisma:generate": "prisma generate",
        "prisma:studio": "prisma studio",
        "prisma:push": "prisma db push",
        "postinstall": "prisma generate",
        "prepare": "husky"
    },
    "dependencies": {
        "@ai-sdk/openai": "^1.0.11",
        "@faker-js/faker": "^9.3.0",
        "@prisma/client": "^5.22.0",
        "@prisma/extension-accelerate": "^1.2.1",
        "ai": "^4.2.5",
        "axios": "^1.7.9",
        "chalk": "^5.4.1",
        "debug": "^4.4.0",
        "dedent": "^1.5.3",
        "discord.js": "^14.17.3",
        "dotenv": "^16.4.7",
        "lru-cache": "^11.0.2",
        "prisma": "^5.22.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@commitlint/cli": "^19.6.0",
        "@commitlint/config-conventional": "^19.6.0",
        "commitlint": "^19.6.0",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.3.0",
        "prettier": "^3.4.2"
    },
    "lint-staged": {
        "./src/**/*.{js,jsx,cjs}": [
            "eslint --fix .",
            "prettier --write ."
        ],
        "./src/**/*.{json,md,yml}": [
            "prettier --write ."
        ]
    }
}
