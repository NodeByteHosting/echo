{
    "name": "@nodebyte/indexie",
    "version": "1.0.0",
    "type": "module",
    "author": "Toxic Dev",
    "main": "src/index.js",
    "scripts": {
        "dev": "nodemon src/index.js",
        "start": "node src/index.js",
        "postinstall": "prisma generate",
        "prepare": "husky"
    },
    "dependencies": {
        "@faker-js/faker": "^9.2.0",
        "@prisma/client": "^5.22.0",
        "chalk": "^5.3.0",
        "debug": "^4.3.7",
        "discord.js": "^14.16.3",
        "dotenv": "^16.4.5",
        "lru-cache": "^11.0.2",
        "module-alias": "^2.2.3",
        "prisma": "^5.22.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.25.9",
        "@babel/preset-env": "^7.26.0",
        "@commitlint/cli": "^19.6.0",
        "@commitlint/config-conventional": "^19.6.0",
        "commitlint": "^19.6.0",
        "eslint": "^8",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "nodemon": "^3.1.7"
    },
    "_moduleAliases": {
        "@": ".",
        "@client": "src/class/client",
        "@classes": "src/class",
        "@configs": "src/configs",
        "@database": "src/prisma/prisma.client",
        "@filters": "src/filters",
        "@functions": "src/functions",
        "@handlers": "src/handlers"
    },
    "lint-staged": {
        "./src/**/*.{js,jsx,cjs}": [
            "eslint --fix .",
            "prettier --write ."
        ],
        "./src/**/*.{json,md,yml}": [
            "prettier --write ."
        ]
    }
}
